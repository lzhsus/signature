<template>
    <view id="signature-contain">
        <canvas class='canvasSignature' canvas-id="canvasSignature" disable-scroll='true' 
            bindtouchmove='move' bindtouchstart='start'
            bindtouchend='end' bindtouchcancel='cancel'
            bindlongtap='tap' binderror='error'>
        </canvas>
        <view class="clear-btn" @tap="clearClick"></view>
        <view class="create-box">
            <view class="close-create-img" @tap="closeClick"></view>
            <view class="is-create-img" @tap="saveClick"></view>
        </view>
    </view>
</template>
<script>
    import wepy from 'wepy'
    export default class signatureContain extends wepy.component {
        data = {
            content:null,
            touchs:[],
            isCreateImg:false
        }
        components = {}
        methods = {
            // 画布的触摸移动开始手势响应
            start (event) {
                // console.log("触摸开始" + event.changedTouches[0].x)
                // console.log("触摸开始" + event.changedTouches[0].y)
                //获取触摸开始的 x,y
                let point = { 
                    x: event.changedTouches[0].x, 
                    y: event.changedTouches[0].y 
                }
                // 
                this.touchs.push(point);
            },
            // 画布的触摸移动手势响应
            move(e) {
                let point = {x: e.touches[0].x, y: e.touches[0].y}
                this.touchs.push(point)
                if(this.touchs.length >= 2) {
                    this.creatDraw()
                }
            },
            // 画布的触摸移动结束手势响应
            end(e){
                console.log("触摸结束",e)
                //清空轨迹数组
                for (let i = 0 ;i < this.touchs.length;i++ ){
                    this.touchs.pop()
                }
                // 至少输入一笔画  可以生成图片
                this.isCreateImg = true;
            },
            // 手指触摸动作被打断，如来电提醒，弹窗	
            cancel(e) {
                console.log("触摸取消"+e)
            },
            // 手指长按 500ms 之后触发，触发了长按事件后进行移动不会触发屏幕的滚动	
            tap(e) {
                console.log("长按手势"+e)
            },
            // 当发生错误时触发 error 事件，detail = {errMsg}	
            error(e){
                console.log("画布触摸错误"+e)
            },
            //清除操作
            clearClick() {
                //清除画布
                this.content.clearRect(0,0,this.content._context.canvas.width,this.content._context.canvas.height)
                this.content.draw(true)
            },
            closeClick(){
                this.$emit('closeClick')
            },
            //生成图片
            saveClick() {
                var that = this
                wx.canvasToTempFilePath({
                    canvasId: 'canvasSignature',
                    success:function(res) {
                    //打印图片路径
                        console.log(res.tempFilePath)
                        //设置保存的图片
                        that.$emit('createImgSuccess',res.tempFilePath)
                    }
                })
            }
        }
        //绘制
        creatDraw() {
            let point1 = this.touchs[0]
            let point2 = this.touchs[1]
            this.touchs.shift()
            this.content.moveTo(point1.x, point1.y)
            this.content.lineTo(point2.x, point2.y)
            this.content.stroke()
            this.content.draw(true)
        }
        events = {
            'createTouchsCanvas':()=>{
                //获得Canvas的上下文
                this.content = wx.createCanvasContext('canvasSignature');
                console.log(this.content._context.canvas.width)
                //设置线的颜色
                this.content.setStrokeStyle("#000")
                //设置线的宽度
                this.content.setLineWidth(2)
                //设置线两端端点样式更加圆润
                this.content.setLineCap('round')
                //设置两条线连接处更加圆润
                this.content.setLineJoin('round')
            }
        }
    }
</script>

<style type="less" scoped>
	#signature-contain{
        width: 100%;height: 100%;
        .canvasSignature{
            width: 100%;height: 100%;display: block;
        }
        .clear-btn{
            background: url(data:image/png;base64,) center center no-repeat;
            background-size: cover;
            width: 50rpx;height: 50rpx;
        }
        .create-box{
            position: fixed;top: 30rpx;right: 30rpx;
            width: 200rpx;height: 50rpx;
            display: flex;justify-content: center;align-items: center;
            .close-create-img{
                background: url(data:image/png;base64,) center center no-repeat;
                background-size: cover;
                width: 50rpx;height: 50rpx;
                margin: 0 12rpx;
            }
            .is-create-img{
                background: url(data:image/png;base64,) center center no-repeat;
                background-size: cover;
                width: 50rpx;height: 50rpx;
            }
        }
        
    }
 
</style>